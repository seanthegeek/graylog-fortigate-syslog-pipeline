        "source": {
          "@type": "string",
          "@value": "rule \"remove unused start action fields\"\nwhen\n  ( has_field(\"action\") &&\nto_string($message.\"action\") == \"start\"\n)\n\nthen\n  remove_field(\n    field : \"duration\",\n    invert : false\n  );\n  remove_field(\n    field : \"rcvdbyte\",\n    invert : false\n  );\n  remove_field(\n    field : \"rcvdpkt\",\n    invert : false\n  );\n  remove_field(\n    field : \"sentbyte\",\n    invert : false\n  );\n  remove_field(\n    field : \"sentpkt\",\n    invert : false\n  );\nend"
        }
      },
      "constraints": [
        {
          "type": "server-version",
          "version": ">=7.0.1+29f3984"
        }
      ]
